<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".My_home">


    <androidx.appcompat.widget.Toolbar
        android:id="@+id/toolbar2"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:background="?attr/colorPrimary"
        android:minHeight="?attr/actionBarSize"
        android:theme="?attr/actionBarTheme"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent">


        <ImageView
            android:id="@+id/imageView3"
            android:layout_width="50dp"
            android:layout_height="50dp"
            android:layout_gravity="right"
            android:paddingRight="10dp"
            app:srcCompat="@drawable/poweroff" />

        <TextView
            android:id="@+id/textView28"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:fontFamily="@font/calistoga"

            android:text="USER HOME"
            android:textColor="@color/black"
            android:textSize="24dp"
            android:textStyle="bold"

            app:layout_constraintStart_toStartOf="parent"
            tools:layout_editor_absoluteY="14dp" />
    </androidx.appcompat.widget.Toolbar>



    <androidx.cardview.widget.CardView
        android:id="@+id/c2"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_margin="15dp"
        android:layout_marginTop="32dp"
        android:elevation="10dp"
        app:cardCornerRadius="15dp"
        app:cardElevation="10dp"
        app:cardMaxElevation="20dp"
        app:layout_constraintTop_toBottomOf="@+id/toolbar2"
        tools:layout_editor_absoluteX="15dp">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_margin="10dp"
            android:elevation="10dp"
            android:orientation="horizontal">

            <ImageView
                android:id="@+id/image2"
                android:layout_width="60dp"
                android:layout_height="60dp"
                android:background="@drawable/restaurant"
                android:scaleType="centerInside" />

            <TextView
                android:id="@+id/text2"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_marginLeft="30dp"
                android:layout_marginBottom="10dp"
                android:background="#00F85C2C"
                android:fontFamily="@font/berkshire_swash"
                android:gravity="center_vertical"
                android:hapticFeedbackEnabled="false"
                android:paddingStart="5dp"
                android:text="Nearest Restaurant"
                android:textColor="@color/black"
                android:textSize="20sp"
                android:textStyle="bold" />
        </LinearLayout>

    </androidx.cardview.widget.CardView>


    <androidx.cardview.widget.CardView
        android:id="@+id/c4"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_margin="15dp"
        android:layout_marginTop="32dp"
        android:elevation="10dp"
        app:cardCornerRadius="15dp"
        app:cardElevation="10dp"
        app:cardMaxElevation="20dp"
        app:layout_constraintTop_toBottomOf="@+id/c2"
        tools:layout_editor_absoluteX="15dp">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_margin="10dp"
            android:elevation="10dp"
            android:orientation="horizontal">

            <ImageView
                android:id="@+id/image4"
                android:layout_width="60dp"
                android:layout_height="60dp"
                android:background="@drawable/resetpassword"
                android:scaleType="centerInside" />

            <TextView
                android:id="@+id/text4"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_marginLeft="30dp"
                android:layout_marginBottom="10dp"
                android:background="#00F85C2C"
                android:gravity="center_vertical"
                android:hapticFeedbackEnabled="false"
                android:text="Change Password"
                android:fontFamily="@font/berkshire_swash"
                android:textColor="@color/black"
                android:textSize="20sp"
                android:textStyle="bold|italic" />
        </LinearLayout>

    </androidx.cardview.widget.CardView>

    <androidx.cardview.widget.CardView
        android:id="@+id/c9"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_margin="15dp"
        android:layout_marginTop="32dp"
        android:elevation="10dp"
        app:cardCornerRadius="15dp"
        app:cardElevation="10dp"
        app:cardMaxElevation="20dp"
        app:layout_constraintTop_toBottomOf="@+id/c4"
        tools:layout_editor_absoluteX="15dp">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_margin="10dp"
            android:elevation="10dp"
            android:orientation="horizontal">

            <ImageView
                android:id="@+id/image9"
                android:layout_width="60dp"
                android:layout_height="60dp"
                android:background="@drawable/continuous"
                android:scaleType="centerInside" />

            <TextView
                android:id="@+id/text9"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_marginLeft="30dp"
                android:layout_marginBottom="10dp"
                android:background="#00F85C2C"
                android:gravity="center_vertical"
                android:hapticFeedbackEnabled="false"
                android:text="Send Feedback"
                android:fontFamily="@font/berkshire_swash"
                android:textColor="@color/black"
                android:textSize="20sp"
                android:textStyle="bold|italic" />
        </LinearLayout>

    </androidx.cardview.widget.CardView>

    <androidx.cardview.widget.CardView
        android:id="@+id/c5"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_margin="15dp"
        android:layout_marginTop="32dp"
        android:elevation="10dp"
        app:cardCornerRadius="15dp"
        app:cardElevation="10dp"
        app:cardMaxElevation="20dp"
        app:layout_constraintTop_toBottomOf="@+id/c9"
        tools:ignore="MissingConstraints"
        tools:layout_editor_absoluteX="15dp">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_margin="10dp"
            android:elevation="10dp"
            android:orientation="horizontal">

            <ImageView
                android:id="@+id/image5"
                android:layout_width="60dp"
                android:layout_height="60dp"
                android:background="@drawable/grocerycart"
                android:scaleType="centerInside" />

            <TextView
                android:id="@+id/text5"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_marginLeft="30dp"
                android:layout_marginBottom="10dp"
                android:background="#00F85C2C"
                android:gravity="center_vertical"
                android:hapticFeedbackEnabled="false"
                android:text="My Cart"
                android:fontFamily="@font/berkshire_swash"
                android:textColor="@color/black"
                android:textSize="20sp"
                android:textStyle="bold|italic" />



        </LinearLayout>

    </androidx.cardview.widget.CardView>

